<edmx:Edmx xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx" Version="1.0">
	<edmx:DataServices xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata" m:DataServiceVersion="2.0">
		<Schema xmlns:d="http://schemas.microsoft.com/ado/2007/08/dataservices"
			xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata" xmlns="http://schemas.microsoft.com/ado/2008/09/edm"
			xmlns:sap="http://www.sap.com/Protocols/SAPData"
			Namespace="SplOvp">
			<EntityType Name="GlobalFilter" sap:content-version="1">
				<Key>
					<PropertyRef Name="RegionID"/>
					<PropertyRef Name="ContractID"/>
				</Key>
				<Property Name="RegionID" sap:label="Регион" Type="Edm.String" Nullable="false"/>
				<Property Name="ContractID" sap:label="№ контракта" Type="Edm.String" Nullable="true"/>
			</EntityType>

			<EntityType Name="RequestsDonut" sap:content-version="1">
				<Key>
					<PropertyRef Name="DonutID"/>
				</Key>
				<Property Name="DonutID" sap:label="Заявка" Type="Edm.String" Nullable="false" sap:creatable="false" sap:updatable="false"/>
				<Property Name="RegionID" sap:label="Регион" Type="Edm.String" Nullable="false" sap:creatable="false" sap:updatable="false"/>
				<Property Name="ContractID" sap:label="№ контракта" Type="Edm.String" Nullable="true" sap:creatable="false" sap:updatable="false"/>
				<Property Name="Type" sap:label="Тип заявки" Type="Edm.String" Nullable="true" MaxLength="6"/>
				<Property Name="Description" sap:label="Описание" sap:aggregation-role="dimension" Type="Edm.String" Nullable="true" MaxLength="20"/>
				<Property Name="Count" sap:label="Количество" sap:aggregation-role="measure" Type="Edm.Int16" Nullable="true"/>
			</EntityType>

			<EntityType Name="ContractDonut" sap:label="Исполнение по договорам" sap:content-version="1">
				<Key>
					<PropertyRef Name="DonutID"/>
				</Key>
				<Property Name="DonutID" sap:label="Сектор ID" Type="Edm.String" Nullable="false" sap:creatable="false"	sap:updatable="false"/>
				<Property Name="RegionID" sap:label="Регион" Type="Edm.String" Nullable="false" sap:creatable="false" sap:updatable="false"/>
				<Property Name="ContractID" sap:label="№ контракта" Type="Edm.String" Nullable="true" sap:creatable="false" sap:updatable="false"/>
				<Property Name="Type" sap:label="Тип заявки" Type="Edm.String" Nullable="true" MaxLength="6"/>
				<Property Name="Description" sap:label="Описание" sap:aggregation-role="dimension" Type="Edm.String" Nullable="true" MaxLength="20"/>
				<Property Name="Value" sap:label="Траты по сектору" sap:aggregation-role="measure" Type="Edm.Int32" Nullable="true"/>
			</EntityType>

			<EntityType Name="Request" sap:content-version="1">
				<Key>
					<PropertyRef Name="RequestID"/>
				</Key>
				<Property Name="RequestID" sap:label="№ заявки" Type="Edm.String" Nullable="false" sap:creatable="false" sap:updatable="false"/>
				<Property Name="Description" sap:label="Описание" Type="Edm.String" MaxLength="120" Nullable="false"/>
				<Property Name="Type" sap:label="Тип заявки" Type="Edm.String" Nullable="true" MaxLength="6"/>
				<Property Name="BeginDate" sap:label="Дата создания" Type="Edm.DateTime" sap:display-format="DateTime" Nullable="false"/>
				<Property Name="Status" sap:label="Статус заявки" Type="Edm.String" Nullable="true"/>
			</EntityType>

			<EntityType Name="RegionVH" sap:content-version="1">
				<Key>
					<PropertyRef Name="RegionID" />
				</Key>
				<Property Name="RegionID" sap:label="Регион ID" Type="Edm.String" Nullable="false" Unicode="true" FixedLength="false"/>
				<Property Name="RegionText" sap:label="Регион" Type="Edm.String" MaxLength="60" Nullable="false"/>
			</EntityType>

			<EntityType Name="ContractVH" sap:content-version="1">
				<Key>
					<PropertyRef Name="ContractID" />
				</Key>
				<Property Name="ContractID" sap:label="№ контракта" Type="Edm.String" Nullable="true" sap:creatable="false" sap:updatable="false"/>
				<Property Name="ContractText" sap:label="Описание" Type="Edm.String" MaxLength="60" Nullable="false"/>
			</EntityType>

			<EntityContainer Name="SplOvpEntities" m:IsDefaultEntityContainer="true">
				<EntitySet Name="RequestsDonutSet" EntityType="SplOvp.RequestsDonut"/>
				<EntitySet Name="ContractDonutSet" EntityType="SplOvp.ContractDonut"/>
				<EntitySet Name="RequestSet" EntityType="SplOvp.Request"/>
				<EntitySet Name="RegionVHSet" EntityType="SplOvp.RegionVH"/>
				<EntitySet Name="ContractVHSet" EntityType="SplOvp.ContractVH"/>
				<EntitySet Name="GlobalFilterSet" EntityType="SplOvp.GlobalFilter"/>
			</EntityContainer>
		</Schema>
	</edmx:DataServices>
</edmx:Edmx>
